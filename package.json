{
  "name": "catco",
  "version": "1.0.1",
  "type": "module",
  "description": "Glob copy file contents to ya clipboard.",
  "scripts": {
    "build": "pkgroll",
    "start": "tsx src/index.ts ./src/**/*.ts"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    "import": {
      "default": "./dist/index.mjs"
    },
    "require": {
      "default": "./dist/index.cjs"
    }
  },
  "bin": {
    "catco": "./dist/index.js"
  },
  "license": "MIT",
  "homepage": "https://github.com/tasteee/catco",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tasteee/catco.git"
  },
  "bugs": "https://github.com/tasteee/catco/issues",
  "author": "tasteink <rocolcleasure@gmail.com>",
  "devDependencies": {
    "pkgroll": "^2.12.2",
    "prettier": "^3.5.3",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  },
  "dependencies": {
    "clipboardy": "^4.0.0",
    "globby": "^14.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "ai",
    "ai-assistant",
    "ai-tools",
    "gpt",
    "copy",
    "clipboard",
    "claude",
    "chatgpt",
    "chatgpt4o",
    "chatgpt4o-mini"
  ]
}
